  <script src="/res/tqfr/js/jquery.dataset.js" type="text/javascript"></script>
  <script type="text/javascript">
  var create_report_detail = function (evt) {
    var idx, len, max, top, lines,
      data = $(this).dataset("values").split(","),
      titles = ["Excellent", "Very Good", "Good", "Fair", "Poor", "N/A"],
      total = $(this).dataset("total");
    len = data.length;
    max = Math.max.apply(null, data),
    loc = {};

    // If you're gonna do percents that add up to 100, you gotta make sure they don't add up to 99.9% or 100.1%.
    percents = [];
    total_percent = 0.0;
    last_percent_idx = 0;
    for (idx = 0; idx < len; idx += 1) {
      percent = Math.round(data[idx] / total * 1000) / 10;
      percents.push(percent);
      total_percent += percent;
      // Keep track of the last non-0 percent value, so we know which one to tweak if they don't add up.
      if (percent) {
        last_percent_idx = idx;
      }
    }
    if (total_percent > 100) {
      percents[last_percent_idx] -= 0.1;
    }
    else if (total_percent < 100) {
      percents[last_percent_idx] += 0.1;
    }

    $("#report_detail_div table").empty();
    for (idx = 0; idx < len; idx += 1) {
      lines = [];
      lines.push("<tr>");
      lines.push("<td><div class='legend_c" + idx + "'></div>");
      lines.push(titles[idx] + "</td>");
      lines.push("<td>Number: " + data[idx] + "</td>");
      // We've already rounded to the nearest 0.1%, but the vagueries of floating point math mean we need to chop anyway.
      lines.push("<td>Percentage: " + percents[idx].toFixed(1) + "%</td>");
      lines.push("</tr>");
      if (max == 0) {
        $("#report_detail_div .c" + idx).css("height", "0px");
      }
      else{
        $("#report_detail_div .c" + idx).css("height", ((data[idx] / max) * 50) + "px");
      }
      $("#report_detail_div table").append(lines.join(""));
    }
    loc = fix_position(evt, 350, 250, 5);
    $("#report_detail_div").css({
      "display": "block",
      "left": loc.x + "px",
      "top": loc.y + "px",
    });
  };

  var destroy_report_detail = function (evt) {
    $("#report_detail_div").css("display", "none");
  };

  var fix_position = function (evt, w, h, margin) {
    var x, y;
    var view_h = $(window).height();
    var view_t = $(document).scrollTop();
    var rel_y = evt.pageY - view_t;
    if ((rel_y + h) > (view_h - margin)) {
      y = evt.pageY - ((rel_y + h) - (view_h - margin));
    } else {
      y = evt.pageY - 16;
    }
    x = evt.pageX + 25;
    return {'x': x, 'y': y};
  }

  var make_report_graph = function (element) {
    var idx, len, max, data = $(element).dataset("values").split(",");
    len = data.length;
    max = Math.max.apply(null, data);
    for (idx = 0; idx < len; idx += 1) {
      if (max == 0) {
        $(element).children(".c" + idx).css("height", "0px");
      }
      else{
        $(element).children(".c" + idx).css("height", ((data[idx] / max) * 25) + "px");
      }
    }
  };

  $(document).ready(function () {
    $("table div.report_graph").each(function () {
      make_report_graph(this);
    }).hover(create_report_detail, destroy_report_detail);
  });
  </script>

